# Docker Environment Variables
# Copy this file to .env for local development with Docker

# PostgreSQL
POSTGRES_USER=aisocial
POSTGRES_PASSWORD=aisocial_password_change_in_production
POSTGRES_DB=ai_social_db

# Application
NODE_ENV=development
JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters-long
NEXTAUTH_SECRET=your-super-secret-nextauth-key-minimum-32-characters-long

# URLs
NEXTAUTH_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3001
CORS_ORIGIN=http://localhost:3000
